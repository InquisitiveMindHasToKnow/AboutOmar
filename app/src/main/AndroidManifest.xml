<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.ohmstheresistance.aboutomar">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ojrappicon"
        android:label="@string/alternate_app_tag"
        android:roundIcon="@drawable/ojrappicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.Resume" android:screenOrientation="portrait" />
        <activity android:name=".activities.Projects" android:screenOrientation="portrait" />
        <activity android:name=".activities.AboutMe" android:screenOrientation="portrait" />
        <activity android:name=".activities.ContactMe" android:screenOrientation="portrait"/>
        <activity android:name=".activities.MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>